<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="true">
    <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindMusicServlet</servlet-name>
        <servlet-class>servlet.FindMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindMusicServlet</servlet-name>
        <url-pattern>/findMusic</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadMusicServlet</servlet-name>
        <servlet-class>servlet.UploadMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadMusicServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadInsertServlet</servlet-name>
        <servlet-class>servlet.UploadInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadInsertServlet</servlet-name>
        <url-pattern>/uploadsucess</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>servlet.DeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/deleteServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteSelMusicServlet</servlet-name>
        <servlet-class>servlet.DeleteSelMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteSelMusicServlet</servlet-name>
        <url-pattern>/deleteSelMusicServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoveMusicServlet</servlet-name>
        <servlet-class>servlet.LoveMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoveMusicServlet</servlet-name>
        <url-pattern>/loveMusicServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindLoveMusicServlet</servlet-name>
        <servlet-class>servlet.FindLoveMusicServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindLoveMusicServlet</servlet-name>
        <url-pattern>/findLoveMusic</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RemoveLoveServlet</servlet-name>
        <servlet-class>servlet.RemoveLoveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RemoveLoveServlet</servlet-name>
        <url-pattern>/removeLoveServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>



</web-app>